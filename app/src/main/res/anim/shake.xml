<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:duration="70"
        android:fromDegrees="-40"
        android:toDegrees="40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:repeatMode="reverse"
        />
    <translate
        android:fromXDelta="-10"
        android:toYDelta="10"
        android:repeatMode="reverse"

        />

</set>